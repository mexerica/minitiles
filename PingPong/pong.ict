    lastmod: 1639351342581
  - name: raquete_2
    depth: 0
    oncreate: |
      this.ctype = "APlayer";
      this.speed = 2;
      this.direction = 360;
      this.tex = "r2";
    onstep: |-
      this.move();

      if (this.x > 448) {
          this.tex = "r2e";
      } else {
          this.tex = "r2";
      }
      if (this.x == ct.room.bolinha){
          this.speed = 0;
      } else {
          if (ct.room.hardmode == true){
              this.speed = 5;
          } else {
              this.speed = 2;
          }
          if (this.x < ct.room.bolinha) { 
              this.direction = 360; 
          } else {
              this.direction = 180; 
          }
      }
