    depth: 0
    oncreate: |-
      this.ctype = "NotPlayer";
      this.tex = "r2";
    onstep: >-
      if (ct.actions.AMoveUp.pressed){
          this.ctype = "BPlayer";
      } else if (ct.actions.AMoveDown.pressed){
          this.ctype = "BPlayerSlow";
      } else if ((ct.actions.AMoveX.pressed) && (ct.actions.AMoveX.value == -1))
      {
          this.ctype = "BPlayerEsq";
      } else if ((ct.actions.AMoveX.pressed) && (ct.actions.AMoveX.value == 1)){
          this.ctype = "BPlayerDir";
      } else {
          this.ctype = "NotPlayer";
      }


      if (this.x > 448) {
          this.tex = "r2e";
      } else {
          this.tex = "r2";
      }


      if (this.x < 0) { 
          this.x = 0; 
      }


      if ((ct.place.free(this, this.x + Math.sign(this.hspeed), this.y,
      'WallsL')) && (ct.place.free(this, this.x + Math.sign(this.hspeed),
      this.y, 'WallsR'))) {
          this.x += 8 * ct.delta * ct.actions.AMoveX.value;
      } else {
          if ((!ct.place.free(this, this.x + Math.sign(this.hspeed), this.y, 'WallsL'))){
              this.x += 1;
          } else {
               this.x -= 1;
          }
      }
