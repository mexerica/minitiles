      this.random;
      this.slow = false;
      this.acelerate = false;
    onstep: >-
      this.move();

      ct.room.bolinha = this.x;


      if (this.y > ct.viewHeight + 80) {
          ct.room.score1 += 1;
          this.kill = true;
      }


      if (!ct.place.free(this, this.x + Math.sign(this.hspeed), this.y,
      'Player')) {
          if (this.direction = 270){
              this.direction = 90;
          } else {
              this.direction = 270;
          }
          this.acelerate = true;
          this.next = true;
      } 


      if (!ct.place.free(this, this.x + Math.sign(this.hspeed), this.y,
      'PlayerSlow')){
          if (this.direction = 270){
              this.direction = 90;
          } 
          this.speed = 3;
          this.next = true;
      }


      if (!ct.place.free(this, this.x + Math.sign(this.hspeed), this.y,
      'PlayerDir')) {
          this.acelerate === true
          this.direction = 45;
          this.next = true;
      } 


      if (!ct.place.free(this, this.x + Math.sign(this.hspeed), this.y,
      'PlayerEsq')) {
          this.acelerate === true
          this.direction = 135;
          this.next = true;
      } 


      if (!ct.place.free(this, this.x + Math.sign(this.hspeed), this.y,
      'APlayer')) {
          if ((this.direction = 90) || (this.direction = 45) || (this.direction = 135)) {
              this.random = Math.random();
              if (this.random < 0.4){
                  this.direction = 270;
              } else if (this.random < 0.7){
                  this.direction = 225;
              } else {
                  this.direction = 315;
              }
          }
          this.acelerate = true;
          this.next = false;
      } 


      if (!ct.place.free(this, this.x + Math.sign(this.hspeed), this.y,
      'WallsD')){
          if (this.direction === 90){
              this.direction = 270;
          }  else if (this.direction === 45){
              this.direction = 315;
          } else if (this.direction === 135){
              this.direction = 225;
          } 
          this.slow = true;
      }


      if (!ct.place.free(this, this.x + Math.sign(this.hspeed), this.y,
      'WallsU')){
          if (this.direction === 270){
              this.direction = 90;
          } else if (this.direction === 315){
              this.direction = 45;
          } else if ((this.direction > 224) && (this.direction < 226)){
              this.direction = 135;
          } 
          this.slow = true;
      }


      if (!ct.place.free(this, this.x + Math.sign(this.hspeed), this.y,
      'WallsL')){
          if (this.direction === 135){
              this.direction = 45;
          }  else {
              this.direction = 315;
          }
          this.slow = true;
      }


      if (!ct.place.free(this, this.x + Math.sign(this.hspeed), this.y,
      'WallsR')){
          if (this.direction === 45){
              this.direction = 135;
          }  else {
              this.direction = 225;
          }
          this.slow = true;
      }


      if (this.slow === true) {
          if (this.speed - 0.1 > 1){
              this.speed -= 0.1;
          } else {
              this.speed = 1;
          }
          this.slow = false;
      }


      if (this.acelerate === true) {
          if (this.speed + 0.25 <= 20){
              this.speed += 0.25;
          } else {
              this.speed = 20;
          }
          this.acelerate = false;
      }


      if (!ct.place.free(this, this.x + Math.sign(this.hspeed), this.y,
      'Change')){
          this.kill = true;
          ct.room.score1 += 1;
          //ct.rooms.switch(ct.room.next)
      }
