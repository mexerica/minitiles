types:
  - name: raquete_1
    depth: 0
    oncreate: |-
      this.ctype = "NotPlayer";
      this.tex = "r1";
    onstep: >-
      if (ct.actions.MoveUp.pressed){
          this.ctype = "Player";
      } else if (ct.actions.MoveDown.pressed){
          this.ctype = "PlayerSlow";
      } else if ((ct.actions.MoveX.pressed) && (ct.actions.MoveX.value == -1)) {
          this.ctype = "PlayerEsq";
      } else if ((ct.actions.MoveX.pressed) && (ct.actions.MoveX.value == 1)){
          this.ctype = "PlayerDir";
      } else {
          this.ctype = "NotPlayer";
      }


      //this.x += 8 * ct.delta * ct.actions.MoveX.value;


      if (this.x > 448) {
          this.tex = "r1e";
      } else {
          this.tex = "r1";
      }


      if (this.x < 0) { 
          this.x = 0; 
      }


      if ((ct.place.free(this, this.x + Math.sign(this.hspeed), this.y,
      'WallsL')) && (ct.place.free(this, this.x + Math.sign(this.hspeed),
      this.y, 'WallsR'))) {
          this.x += 8 * ct.delta * ct.actions.MoveX.value;
      } else {
          if ((!ct.place.free(this, this.x + Math.sign(this.hspeed), this.y, 'WallsL'))){
              this.x += 1;
          } else {
               this.x -= 1;
          }
      }
    ondraw: ''
    ondestroy: ''
    texture: 8cf55d7c-f46b-4d28-a4c3-ca40756b778d
    extends: {}
    uid: b641f1ec-daae-4654-bb45-9d7105f2010a
    lastmod: 1639348923945
  - name: Ball
    depth: 0
    oncreate: |-
      this.ctype = "Ball";
      this.speed = 3;
      this.direction = 270;
      this.next = false;
      this.saveX = this.x;
      this.saveY = this.y;
      this.random;
      this.slow = false;
      this.acelerate = false;
    onstep: >-
      this.move();

      ct.room.bolinha = this.x;


      if (this.y > ct.viewHeight + 80) {
          this.x = this.saveX; 
          this.y = this.saveY; 
          this.speed = 3;
          ct.room.score1 += 1;
          console.log(ct.room.score1);
          //this.kill = true;
      }


      if (!ct.place.free(this, this.x + Math.sign(this.hspeed), this.y,
      'Player')) {
          if (this.direction = 270){
              this.direction = 90;
          } else {
              this.direction = 270;
          }
          this.acelerate = true;
          this.next = true;
      } 


      if (!ct.place.free(this, this.x + Math.sign(this.hspeed), this.y,
      'PlayerSlow')){
          if (this.direction = 270){
              this.direction = 90;
          } 
          this.speed = 3;
          this.next = true;
      }


      if (!ct.place.free(this, this.x + Math.sign(this.hspeed), this.y,
      'PlayerDir')) {
          this.acelerate === true
          this.direction = 45;
          this.next = true;
      } 


      if (!ct.place.free(this, this.x + Math.sign(this.hspeed), this.y,
      'PlayerEsq')) {
          this.acelerate === true
          this.direction = 135;
          this.next = true;
      } 


      if (!ct.place.free(this, this.x + Math.sign(this.hspeed), this.y,
      'APlayer')) {
          if ((this.direction = 90) || (this.direction = 45) || (this.direction = 135)) {
              this.random = Math.random();
              if (this.random < 0.4){
                  this.direction = 270;
              } else if (this.random < 0.7){
                  this.direction = 225;
              } else {
                  this.direction = 315;
              }
          }
          this.acelerate = true;
          this.next = false;
      } 


      if (!ct.place.free(this, this.x + Math.sign(this.hspeed), this.y,
      'WallsD')){
          if (this.direction === 90){
              this.direction = 270;
          }  else if (this.direction === 45){
              this.direction = 315;
          } else if (this.direction === 135){
              this.direction = 225;
          } 
          this.slow = true;
      }


      if (!ct.place.free(this, this.x + Math.sign(this.hspeed), this.y,
      'WallsU')){
          if (this.direction === 270){
              this.direction = 90;
          } else if (this.direction === 315){
              this.direction = 45;
          } else if ((this.direction > 224) && (this.direction < 226)){
              this.direction = 135;
          } 
          this.slow = true;
      }


      if (!ct.place.free(this, this.x + Math.sign(this.hspeed), this.y,
      'WallsL')){
          if (this.direction === 135){
              this.direction = 45;
          }  else {
              this.direction = 315;
          }
          this.slow = true;
      }


      if (!ct.place.free(this, this.x + Math.sign(this.hspeed), this.y,
      'WallsR')){
          if (this.direction === 45){
              this.direction = 135;
          }  else {
              this.direction = 225;
          }
          this.slow = true;
      }


      if (this.slow === true) {
          if (this.speed - 0.1 > 1){
              this.speed -= 0.1;
          } else {
              this.speed = 1;
          }
          this.slow = false;
      }


      if (this.acelerate === true) {
          if (this.speed + 0.25 <= 20){
              this.speed += 0.25;
          } else {
              this.speed = 20;
          }
          this.acelerate = false;
      }


      if (!ct.place.free(this, this.x + Math.sign(this.hspeed), this.y,
      'BPlayer')) {
          if (this.direction = 90){
              this.direction = 270;
          } else {
              this.direction = 90;
          }
          this.acelerate = true;
          this.next = true;
      } 


      if (!ct.place.free(this, this.x + Math.sign(this.hspeed), this.y,
      'BPlayerSlow')){
          if (this.direction = 90){
              this.direction = 270;
          } 
          this.speed = 3;
          this.next = true;
      }


      if (!ct.place.free(this, this.x + Math.sign(this.hspeed), this.y,
      'BPlayerDir')) {
          this.acelerate === true
          this.direction = 315;
          this.next = true;
      } 


      if (!ct.place.free(this, this.x + Math.sign(this.hspeed), this.y,
      'BPlayerEsq')) {
          this.acelerate === true
          this.direction = 225;
          this.next = true;
      }


      if (!ct.place.free(this, this.x + Math.sign(this.hspeed), this.y,
      'Change')){
          this.x = this.saveX; 
          this.y = this.saveY; 
          this.speed = 3;
          ct.room.score2 += 1;
      }


      if (ct.room.score1 >= 11){
          ct.rooms.switch(ct.room.next)
      }


      if (ct.room.score2 >= 11){
          ct.rooms.switch(ct.room.anext)
      }
    ondraw: ''
    ondestroy: ''
